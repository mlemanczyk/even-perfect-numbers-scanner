{
  "version": "0.2.0",
  "inputs": [
    {
      "id": "projectName",
      "type": "pickString",
      "description": "Select project to run",
      "options": [
        "EvenPerfectBitScanner",
        "EvenPerfectBitScanner.ResultsParser",
        "EvenPerfectBitScanner.Benchmarks"
      ],
      "default": "EvenPerfectBitScanner"
    }
  ],
  "configurations": [
    {
      "name": "Debug selected project (F5)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Build Debug",
      "program": "${workspaceFolder}/${input:projectName}/bin/Debug/net8.0/${input:projectName}.dll",
			"args": [
				"--increment=add",
				"--mersenne=bydivisor",
				"--threads=1",
				"--gpu-scan-batch=256000",
				"--primes-device=gpu",
				"--mersenne-device=gpu",
				"--mod-reduction=barrett128",
				"--block-size=1000000",
				"--gpu-prime-threads=64",
				"--write-batch-size=10000",
				"--divisor-cycles-device=gpu",
				"--prime=31",
				"--results-prefix=test-",
				"--filter-p=sorted-primes-passed-residue-_even_perfect_bit_scan_inc-add_thr-16_blk-10000000_mers-incremental_mersdev-gpu_ntt-reference_red-barrett128_primesdev-gpu_order-off_orderdev-gpu_gcd-off_modfix-off_gputh-64_llslice-32_scanb-512000_warm-5000000.csv"
			],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "Run selected project (Alt+F5)",
      "type": "coreclr",
      "request": "launch",
      "noDebug": true,
      "preLaunchTask": "Build Release",
      "program": "${workspaceFolder}/${input:projectName}/bin/Release/net8.0/${input:projectName}.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole",
      "internalConsoleOptions": "neverOpen",
      "justMyCode": false
    },
    {
      "name": "Run EvenPerfectBitScanner",
      "type": "coreclr",
      "request": "launch",
      "noDebug": true,
      "preLaunchTask": "Build EvenPerfectBitScanner Release",
      "program": "${workspaceFolder}/EvenPerfectBitScanner/bin/Release/net8.0/EvenPerfectBitScanner.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "justMyCode": false
    },
    {
      "name": "Run EvenPerfectBitScanner.ResultsParser",
      "type": "coreclr",
      "request": "launch",
      "noDebug": true,
      "preLaunchTask": "Build EvenPerfectBitScanner.ResultsParser Release",
      "program": "${workspaceFolder}/EvenPerfectBitScanner.ResultsParser/bin/Release/net8.0/EvenPerfectBitScanner.ResultsParser.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "justMyCode": false
    },
    {
      "name": "Debug EvenPerfectBitScanner",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Build EvenPerfectBitScanner Debug",
      "program": "${workspaceFolder}/EvenPerfectBitScanner/bin/Debug/net8.0/EvenPerfectBitScanner.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "justMyCode": false
    },
    {
      "name": "Debug EvenPerfectBitScanner.ResultsParser",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Build EvenPerfectBitScanner.ResultsParser Debug",
      "program": "${workspaceFolder}/EvenPerfectBitScanner.ResultsParser/bin/Debug/net8.0/EvenPerfectBitScanner.ResultsParser.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "justMyCode": false
    },
    {
      "name": "Run EvenPerfectBitScanner.Benchmarks",
      "type": "coreclr",
      "request": "launch",
      "noDebug": true,
      "preLaunchTask": "Build EvenPerfectBitScanner.Benchmarks Release",
      "program": "${workspaceFolder}/EvenPerfectBitScanner.Benchmarks/bin/Release/net8.0/EvenPerfectBitScanner.Benchmarks.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "justMyCode": false
    },
    {
      "name": "Debug EvenPerfectBitScanner.Benchmarks",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Build EvenPerfectBitScanner.Benchmarks Debug",
      "program": "${workspaceFolder}/EvenPerfectBitScanner.Benchmarks/bin/Debug/net8.0/EvenPerfectBitScanner.Benchmarks.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "justMyCode": false
    }
  ]
}